    "build": "react-router build",
    "dev": "react-router dev",
    "deubg": "NODE_OPTIONS=\"--inspect-brk\" npm run dev",
    "profile": "remix vite:build --profile",
    "start": "react-router-serve ./build/server/index.js",
    "start:powershell": "powershell -Command \"$dir = Get-ChildItem -Path './build/server' -Directory | Select-Object -First 1; react-router-serve ./build/server/$($dir.Name)/index.js\"",
    "test": "vitest",
    "typecheck": "react-router typegen && tsc",
    "update-lexical": "npm install lexical@latest @lexical/dragon@latest @lexical/history@latest @lexical/react@latest @lexical/rich-text@latest @lexical/utils@latest @lexical/file@latest",
    "update-uno": "npm install @unocss/reset@latest unocss@latest unocss-preset-animations@latest unocss-preset-antd@latest",
    "upgrade-remix": "npx upgrade-remix",
    "generate-database-types": "supabase gen types typescript --project-id oclmlxhybvqbomhjeuhu --schema public --debug > app/util/supabase.type.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
